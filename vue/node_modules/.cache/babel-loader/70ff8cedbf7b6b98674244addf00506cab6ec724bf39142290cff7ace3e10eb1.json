{"ast":null,"code":"import { defineComponent } from 'vue';\nexport default {\n  name: 'register',\n  props: {\n    msg: String\n  },\n  data() {\n    return {\n      name: \"\",\n      password: \"\",\n      mail: \"\",\n      tel: \"\"\n    };\n  },\n  methods: {\n    //点击完成按钮触发handlefinish\n    handlefinish: function () {\n      postRequest('register/', {\n        user_id: this.name,\n        user_password: this.password,\n        user_Email: this.mail,\n        user_phone: this.tel\n      }).then(response => {\n        var is_t = response.data;\n      });\n      if (is_t['register'] === this.name) {\n        alert(\"用户名已存在\"); //如果用户名已存在则无法注册\n      } else if (this.name === '') {\n        alert(\"用户名不能为空\");\n      } else {\n        //将新用户信息存储到localStorage\n        sessionStorage.setItem('name', this.name);\n        sessionStorage.setItem('password', this.password);\n        sessionStorage.setItem('mail', this.mail);\n        sessionStorage.setItem('tel', this.tel);\n        sessionStorage.setItem('s', \"false\");\n        alert(\"注册成功\");\n        this.$router.replace('/'); //完成注册后跳转至登录页面\n      }\n    }\n  }\n};","map":{"version":3,"names":["defineComponent","name","props","msg","String","data","password","mail","tel","methods","handlefinish","postRequest","user_id","user_password","user_Email","user_phone","then","response","is_t","alert","sessionStorage","setItem","$router","replace"],"sources":["src/views/register.vue"],"sourcesContent":["<script>\r\nimport {defineComponent} from 'vue'\r\n\r\nexport default {\r\n  name:'register',\r\n  props: {\r\n    msg: String\r\n  },\r\n  data(){\r\n    return{\r\n      name:\"\",\r\n      password:\"\",\r\n      mail:\"\",\r\n      tel:\"\"\r\n    };\r\n  },methods:{\r\n    //点击完成按钮触发handlefinish\r\n    handlefinish:function()\r\n    {\r\n      postRequest('register/',{user_id:this.name,user_password:this.password,user_Email:this.mail,user_phone:this.tel}).then(response=>{\r\n        var is_t = response.data;\r\n\r\n      })\r\n      if(is_t['register']===this.name)\r\n      {\r\n        alert(\"用户名已存在\");//如果用户名已存在则无法注册\r\n      }\r\n      else if(this.name==='')\r\n      {\r\n        alert(\"用户名不能为空\");\r\n      }\r\n      else{//将新用户信息存储到localStorage\r\n        sessionStorage.setItem('name',this.name);\r\n        sessionStorage.setItem('password',this.password);\r\n        sessionStorage.setItem('mail',this.mail);\r\n        sessionStorage.setItem('tel',this.tel);\r\n        sessionStorage.setItem('s',\"false\");\r\n        alert(\"注册成功\");\r\n        this.$router.replace('/');//完成注册后跳转至登录页面\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div id=\"background\">\r\n    <div id=\"contain\">\r\n      <h1>注册</h1>\r\n\r\n      <div class=\"form\">\r\n        <label>用户名：</label><input type=\"text\" v-model.trim=\"name\"><br/>\r\n      </div>\r\n      <div class=\"form\">\r\n        <label>密码：</label><input type=\"password\" v-model.trim=\"password\"><br/>\r\n      </div>\r\n      <div class=\"form\">\r\n        <label>邮箱：</label><input type=\"email\" v-model.trim=\"mail\"><br/>\r\n      </div>\r\n      <div class=\"form\">\r\n        <label>手机号：</label><input type=\"tel\" v-model.trim=\"tel\"><br/>\r\n      </div>\r\n      <button @click.prevent=\"handlefinish\">提交</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n#background{\r\n  width: 100%;\r\n  height: 100%;\r\n  background: url(\"../assets/it.jpg\");\r\n  background-size:100% 100%;\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n}\r\n#contain{\r\n  width: 580px;\r\n  height: 560px;\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%,-50%);\r\n  background:#00000090;\r\n  text-align: center;\r\n  border-radius: 20px;\r\n}\r\n#contain h1{\r\n  color: white;\r\n}\r\n.form{\r\n  color: white;\r\n  margin-left: 20%;\r\n  margin-top: 60px;\r\n  font-size: 20px;\r\n  text-align: left;\r\n}\r\nlabel{\r\n  float:left;\r\n  width: 5em;\r\n  margin-right: 1em;\r\n  text-align: right;\r\n}\r\ninput,textarea{\r\n  margin-left: 10px;\r\n  padding: 4px;\r\n  border: solid 1px #4e5ef3;\r\n  outline: 0;\r\n  font: normal 13px/100% Verdana,Tahoma,sans-serif;\r\n  width: 200px;\r\n  height: 20px;\r\n  background:#f1f1f190;\r\n  box-shadow: rgba(0, 0, 0, 0.1) 0px 0px 8px;\r\n}\r\ninput:hover,textarea:hover,input:focus,textarea:focus{border-color:#0d0aa1;}\r\nbutton{\r\n  position: relative;\r\n  height: 33px;\r\n  width: 150px;\r\n  background: rgba(35, 19, 252, 0.425);\r\n  border-radius: 10px;\r\n  margin-top: 38px;\r\n  box-shadow: none;\r\n  color: white;\r\n  margin-left: 40px;\r\n}\r\n</style>"],"mappings":"AACA,SAAAA,eAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACAC,GAAA,EAAAC;EACA;EACAC,KAAA;IACA;MACAJ,IAAA;MACAK,QAAA;MACAC,IAAA;MACAC,GAAA;IACA;EACA;EAAAC,OAAA;IACA;IACAC,YAAA,WAAAA,CAAA,EACA;MACAC,WAAA;QAAAC,OAAA,OAAAX,IAAA;QAAAY,aAAA,OAAAP,QAAA;QAAAQ,UAAA,OAAAP,IAAA;QAAAQ,UAAA,OAAAP;MAAA,GAAAQ,IAAA,CAAAC,QAAA;QACA,IAAAC,IAAA,GAAAD,QAAA,CAAAZ,IAAA;MAEA;MACA,IAAAa,IAAA,sBAAAjB,IAAA,EACA;QACAkB,KAAA;MACA,OACA,SAAAlB,IAAA,SACA;QACAkB,KAAA;MACA,OACA;QAAA;QACAC,cAAA,CAAAC,OAAA,cAAApB,IAAA;QACAmB,cAAA,CAAAC,OAAA,kBAAAf,QAAA;QACAc,cAAA,CAAAC,OAAA,cAAAd,IAAA;QACAa,cAAA,CAAAC,OAAA,aAAAb,GAAA;QACAY,cAAA,CAAAC,OAAA;QACAF,KAAA;QACA,KAAAG,OAAA,CAAAC,OAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}